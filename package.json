{
  "name": "drims-be",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint .",
    "build": "npm test",
    "pm2:start": "pm2 describe drims-be > /dev/null 2>&1 || pm2 start src/server.js --name drims-be",
    "pm2:stop": "pm2 describe drims-be > /dev/null 2>&1 && pm2 stop drims-be || echo 'Process not running'",
    "pm2:restart": "pm2 describe drims-be > /dev/null 2>&1 && pm2 restart drims-be || npm run pm2:start",
    "pm2:delete": "pm2 describe drims-be > /dev/null 2>&1 && pm2 delete drims-be || echo 'Process not found'",
    "pm2:logs": "pm2 logs drims-be",
    "pm2:monitor": "pm2 monit drims-be",
    "production:start": "NODE_ENV=production npm run pm2:start",
    "production:stop": "npm run pm2:stop",
    "production:restart": "npm run pm2:restart",
    "preproduction:start": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "csv-generate": "^4.4.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-prom-bundle": "^7.0.0",
    "helmet": "^7.1.0",
    "instagram-private-api": "^1.46.1",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.3",
    "tough-cookie": ">=4.1.3",
    "twitter-api-v2": "^1.17.2",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "eslint": "^9.10.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "pm2": "^5.4.2",
    "supertest": "^7.0.0",
    "tough-cookie": ">=4.1.3"
  }
}